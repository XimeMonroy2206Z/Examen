#include <stdio.h>

int main ()
{
    /* Ximena Monroy Gutiérrez */

    float a, b, c;
    printf("\n\n Ximena Monroy Gutiérrez");
    printf("\n\n Programa 2");
    printf("\n\n Suma de numeros enteros");
    printf("\n\n Dame el valor entero de a:");
